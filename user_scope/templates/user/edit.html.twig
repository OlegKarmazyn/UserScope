{% extends 'base.html.twig' %}

{% block title %}Edit users data{% endblock %}

{% block body %}
    <h1>Edit users data</h1>
    
{{ form_start(form) }}
        {{ form_row(form.username) }}
        {{ form_row(form.password, { 'label': 'Password (optional to edit)' }) }}
        {{ form_row(form.roles) }}
        <button type="submit">Save</button>
    {{ form_end(form) }}
{% endblock %}